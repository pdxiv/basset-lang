10 REM Test compact IF syntax without THEN keyword
20 X=5
30 IF X=5 PRINT "X IS FIVE"
40 IF X=10 PRINT "X IS TEN"
50 Y=3
60 IF Y<5 PRINT "Y IS LESS THAN 5"
70 IF Y>5 PRINT "Y IS GREATER THAN 5"
80 Z=7
90 IF Z=7 GOTO 110
100 PRINT "SKIPPED"
110 PRINT "AFTER IF GOTO"
120 A=1:B=1
130 IF A=1 IF B=1 PRINT "BOTH ARE 1"
140 IF A=2 IF B=1 PRINT "NOT PRINTED"
150 C=10
160 IF C=10 PRINT "C IS 10":PRINT "STILL IN IF"
170 END
