10 REM Test ERR with multiple error types
20 DIM A(5)
30 DATA "Test1","Test2"
40 TRAP 1000
50 PRINT "Test 1: Division by zero"
60 X = 1 / 0
70 PRINT "FAIL: Should have trapped": END
1000 REM Error handler
1010 IF ERR = 11 THEN PRINT "  ERR=11 DIVISION BY ZERO": GOTO 2000
1020 IF ERR = 13 THEN PRINT "  ERR=13 TYPE MISMATCH": GOTO 3000
1030 IF ERR = 9 THEN PRINT "  ERR=9 SUBSCRIPT OUT OF RANGE": GOTO 4000
1040 IF ERR = 1 THEN PRINT "  ERR=1 NEXT WITHOUT FOR": GOTO 5000
1050 IF ERR = 4 THEN PRINT "  ERR=4 OUT OF DATA": GOTO 6000
1060 IF ERR = 5 THEN PRINT "  ERR=5 ILLEGAL FUNCTION": GOTO 7000
1070 PRINT "  Unknown error code"; ERR
1080 END
2000 TRAP 1000
2010 PRINT "Test 2: Type mismatch"
2020 Y = ASC(999)
2030 PRINT "FAIL": END
3000 TRAP 1000
3010 PRINT "Test 3: Array bounds"
3020 Z = A(99)
3030 PRINT "FAIL": END
4000 TRAP 1000
4010 PRINT "Test 4: NEXT without FOR"
4020 NEXT I
4030 PRINT "FAIL": END
5000 TRAP 1000
5010 PRINT "Test 5: OUT OF DATA"
5020 READ A$, B$, C$
5030 PRINT "FAIL": END
6000 TRAP 1000
6010 PRINT "Test 6: SQR of negative"
6020 W = SQR(-5)
6030 PRINT "FAIL": END
7000 PRINT "=== All multiple error tests passed ==="
7010 END
